
  <h1>Sesiones paciente <%=getPacienteName(@paciente) %></h1>


  <table>
    <thead>
      <tr>
        <th>Numero Sesion</th>
        <th>Peso</th>
        <th>Estatura</th>
        <th>Grasa</th>
        <th>Musculo</th>
        <th>Agua</th>
        <th>IMC</th>
        <th>Grasa Vis</th>
        <th>Edad Metabolica</th>
        <th>Hora</th>
        <th>Nutriologo</th>
        <th colspan="11"></th>
      </tr>
    </thead>
  <% numS=1%>
    <tbody>
      <% @sesions.each do |sesion| %>

        <tr>
          <td><%=numS%></td>
          <td><%= sesion.peso %></td>
          <td><%= sesion.estatura %></td>
          <td><%= sesion.grasa %></td>
          <td><%= sesion.musculo %></td>
          <td><%= sesion.agua %></td>
          <td><%= sesion.imc %></td>
          <td><%= sesion.grasa_vis %></td>
          <td><%= sesion.edad_metabolica %></td>
          <td><%= getHora(sesion.hora_id) %></td>
          <td><%=getNutriologoName(sesion.nutriologo_id)%></td>
          <td><%= link_to 'Editar datos', edit_sesion_path(sesion) %></td>
          <td><%= link_to 'Ver Metas Cita', "/metas_cita/"+sesion.id.to_s() %></td>

        </tr>
        <% numS+=1%>
      <% end %>
    </tbody>
  </table>


  <td><%= link_to 'Ver Metas Paciente', "/metas_paciente/"+@paciente.to_s() %></td>

  <br>
